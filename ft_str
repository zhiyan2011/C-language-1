char	*ft_strstr(char *str, char *to_find)
{
int i;
int j;
int start;
i =0;
j = 0ï¼›
if(to_find[0] == '\0')
return str;
	while (str[i])
	{
		start = i;
		j = 0;
		while (to_find[j] && str[start +j] == to_find[j])
		{
			
			j++;
		}
		if (to_find[j] == '\0')
			return &str[start];
		i++; 
	}
	return NULL;
}
